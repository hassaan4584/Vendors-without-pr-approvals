






<div class="panel-group" id="accordion">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                    Profile
                </a>
            </h4>
        </div>
        <div id="collapseOne" class="panel-collapse collapse in">
            <div class="panel-body">
                <div class="list-group">
                    <div class="list-group-item">@Html.ActionLink("View Profile", "MyProfile", "cpuMembers")</div>
                    <div class="list-group-item">@Html.ActionLink("Edit Profile", "About", "Home")</div>
                    <div class="list-group-item">@Html.ActionLink("Logout", "Contact", "Home")</div>

                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                   Meeting
                </a>
            </h4>
        </div>
        <div id="collapseTwo" class="panel-collapse collapse">
            <div class="panel-body">
                <div class="list-group">
                     <div class="list-group-item">@Html.ActionLink("View PR forr Meeting", "view_pr_for_meeting", "cpuMembers")</div>
                     <div class="list-group-item">@Html.ActionLink("View Attending Members", "view_attending_members", "cpuMembers")</div>
                     <div class="list-group-item">@Html.ActionLink("Respond To a meeting", "respond_to_meeting", "cpuMembers")</div>
                     <div class="list-group-item">@Html.ActionLink("Suggest PR for meeeting", "suggest_pr_for_meeting", "cpuMembers")</div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree">
                    Reports
                </a>
            </h4>
        </div>
        <div id="collapseThree" class="panel-collapse collapse">
            <div class="panel-body">
                <div class="list-group">
                    <div class="list-group-item">@Html.ActionLink("View Reports", "Index", "Home")</div>
                    <div class="list-group-item">@Html.ActionLink("Print Report", "About", "Home")</div>
                    
                </div>
            </div>
        </div>
    </div>
</div>
